<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>petify</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>s1</servlet-name>
  <servlet-class>com.swastik.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>s1</servlet-name>
  <url-pattern>/register</url-pattern>
  </servlet-mapping>

<servlet>
  <servlet-name>s6</servlet-name>
  <servlet-class>com.swastik.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>s6</servlet-name>
  <url-pattern>/log</url-pattern>
  </servlet-mapping> 
  
  <servlet>
   <servlet-name>s5</servlet-name>
  <servlet-class>com.swastik.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>s5</servlet-name>
  <url-pattern>/logout</url-pattern>
  </servlet-mapping>  
  
  <servlet>
   <servlet-name>s8</servlet-name>
  <servlet-class>com.swastik.Info</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>s8</servlet-name>
  <url-pattern>/contact</url-pattern>
  </servlet-mapping>  
  
</web-app>